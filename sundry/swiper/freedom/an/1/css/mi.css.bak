/* 设置通用样式 */
*{
	margin:0;
	padding:0;
	border:0;
    outline:0;
    font-size:12px;
    vertical-align:baseline;
    background:transparent;
}
ul,ol{list-style:none;}
a{text-decoration:none;}
table{
	border-collapse:collapse;
}
input,select{
	vertical-align:middle;
}
hr {
    display:block;
    height:1px;
    border:0;   
    border-top:1px solid #ccc;
    margin:1em 0;
    padding:0;
}
q:before, q:after {
	content: ''
}
.lt{float:left;}
.rt{float:right;}
.footer-service>ul:after,.footer-link:after,.footer-middle:after,#banner:after,#banner>.banner-middle:after{
	 content:"";
	 display:block;
	 clear:both;
}
/***********************设置顶部样式 **********************/
#top{
	width:100%;
	background:#333;
	color:#B0B0B0;
	height:40px;
}
.container{
	width:1226px;
	margin:0 auto;
}
#top>.container>ul{
	height:40px;
}
#top>.container>ul>li{
	float:left;
	padding:14px 0;
}
#top>.container a{
	color:#B0B0B0;
}
#top>.container a:hover{
	color:#fff;
}
#top>.container b{
	margin:0 5px 0 4px;
	color:#424242;
}
#top .container .cart{
	height:40px;
	width:120px;
	margin-left:15px;
	position:relative;
}
#top .cart-menu{
	width:316px;
	padding:35px 0 25px;
	position:absolute;
	top:40px;
	left:-197px;
	display:none;
	z-index:300;
	background:#fff;
	border-top:1px solid  transparent;
	box-shadow: 0 2px 10px ;
}
#top .cart-menu>div{
	margin: 5px 20px 20px;
    text-align: center;	
}
#top .container .cart>a{
	background:#424242;
	height:40px;
	line-height:40px;
	display:block;
	text-align:center;
	color:#fff;
