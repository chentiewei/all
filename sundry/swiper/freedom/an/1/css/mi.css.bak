/**************banner部分****************/
#banner{
	position:relative;
}
#banner>ul.lt{
	box-sizing:border-box;
	height:460px;
	padding-top:23px;
	background:rgba(0,0,0,0.3);
	position:absolute;
	top:0;
	left:0;
	z-index:5;
}
#banner>ul.lt>li{
	position:relative;
	
}
#banner>ul.lt>li.hover{
	position:relative;
	background:#FF6700;
	
}
#banner>ul.lt>li>a{
	box-sizing:border-box;
	color:#fff;
	font-size:15px;
	display:inline-block;
	height:40px;
	line-height:40px;
	font-weight:bold;
	width:234px;
	padding-left:30px;
}
#banner .kinds{
	position:absolute;
	top:-23px;
	right:-265px;
	background:#fff;
	z-index:100;
	display:none;
}
#banner .kinds ul{
	height:460px;
	
}
#banner .kinds a:nth-child(1){
	float:left;
	box-sizing:border-box;
	display:inline-block;
	padding-left:20px;
	padding-right:10px;
	margin:17.5px 0;
	width:195px;
	color:#333;
}
#banner .kinds a:nth-child(1):hover{
	color:#FF6700;
}

#banner .kinds a:nth-child(1) span{
	font-size:15px;
	line-height:40px;
	float:left;
}
#banner .kinds a:nth-child(1) img{
	float:left;
}
#banner .kinds a:nth-child(2){
	float:left;
	display:inline-block;
	margin:25px 10px 22px 0;
	width:60px;
	height:23px;
	line-height:23px;
	font-size:16px;
	border:1px solid #FF6700;
	text-align:center;
	color:#FF6700;
}
#banner .kinds a:nth-child(2):hover{
	background:#FF6700;
	color:#fff;
}

/******************banner部分轮播********************/
#banner .play{
	height:460px;
	position:relative;
	overflow:hidden;	
	cursor:pointer;
}
#banner .play ul{
	width:6130px;
	height:460px;
	position:absolute;
	top:0;
	left:0px;
	z-index:3;
	
}
#index{
	position:absolute;
	bottom:25px;
	right:35px;
	z-index:10;
}
#index span{
	display:inline-block;
	width:6px;
	height:6px;
	border-radius:5px;
	border:2px solid #ccc;
	margin-left:10px;
	background:rgba(0,0,0,0.5);
}
#index span.active{
	background:#ccc;
}
#index span:hover{
	background:#fff;
}
#banner .play ul li{
	float:left;
}
#banner .play>span{
	display:inline-block;
	font-size:50px;
	color:red;
	text-align:center;
	width:40px;
	height:60px;
	color:#D9D3C7;
}
#banner .play>span:hover{
	background:rgba(0,0,0,0.3);
}
.play>span:nth-child(3){
	left:234px;
	top:205px;
	position:absolute;
	z-index:10;
}
.play>span:nth-child(4){
	right:0px;
	top:205px;
	position:absolute;
	z-index:10;
}




/******************banner-middle********************/
.banner-middle{
	margin-top:15px;
}
.banner-middle ul.lt li{
	float:left;
	overflow:hidden;
	height:85px;
}
.banner-middle ul.lt{
	width:234px;
	box-sizing:border-box;
}

.banner-middle ul.lt a{
	display:inline-block;
	width:78px;
	height:85px;
	background:#5F5750;
	color:#CDCAC8;
	font-size:13px;
	text-align:center;
}
.banner-middle ul.lt a:hover{
	color:#fff;
}
.banner-middle>a{
	float:left;
	padding-left:14.5px;
}
.banner-middle>a>img{
	width:316px;
}
.banner-middle>a>img:hover{
	box-shadow:0px 15px 15px #efefef;
}
